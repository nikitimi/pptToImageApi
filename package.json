    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^4.0.379",
    "puppeteer": "^22.6.3",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/shelljs": "^0.8.15",
    "typescript": "^5.4.3"
  }
